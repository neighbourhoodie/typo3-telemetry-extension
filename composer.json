{
  "name": "neighbourhoodie/telemetry",
  "description": "Telemetry extension for TYPO3",
  "type": "typo3-cms-extension",
  "license": "GPL-2.0-or-later",
  "autoload": {
    "psr-4": {
      "Neighbourhoodie\\Telemetry\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "telemetry"
    }
  },
  	"config": {
		"allow-plugins": {
			"php-http/discovery": true,
			"tbachert/spi": true
		}
	},
  "require": {
    "typo3/cms-core": "^13.0",
	  "open-telemetry/exporter-otlp": "^1.3",
	  "open-telemetry/sdk": "^1.7"
  }
}
